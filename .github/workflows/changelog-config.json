{
    "template": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n\nThe format is based on [Keep a Changelog](https://keepachangelog.com/en/1.0.0/),\nand this project adheres to [Semantic Versioning](https://semver.org/spec/v2.0.0.html).\n\n#{{CHANGELOG}}",
    "pr_template": "- #{{TITLE}} (#{{NUMBER}})",
    "categories": [
        {
            "title": "## [#{{TO_TAG}}] - #{{TO_TAG_DATE}}\n### üöÄ Added",
            "labels": [
                "feature",
                "feat",
                "enhancement"
            ]
        },
        {
            "title": "### üêõ Fixed",
            "labels": [
                "bug",
                "fix",
                "bugfix"
            ]
        },
        {
            "title": "### üìö Documentation",
            "labels": [
                "documentation",
                "docs"
            ]
        },
        {
            "title": "### üîß Other Changes",
            "labels": []
        }
    ],
    "ignore_labels": [
        "duplicate",
        "invalid",
        "wontfix"
    ],
    "sort": "ASC",
    "max_tags_to_fetch": 10,
    "max_pull_requests": 50
}